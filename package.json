{
  "name": "go-slang",
  "description": "golang in Typescript",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/CS4215-Jikun-Kaian/go-slang.git"
  },
  "private": false,
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "antlr4ts-parser": "antlr4ts -visitor src/lang/GoParser.g4",
    "antlr4ts-lexer": "antlr4ts -visitor src/lang/GoLexer.g4",
    "build": "tsc --build",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "antlr4": "^4.13.1-patch-1",
    "antlr4ts": "^0.5.0-alpha.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript-jest": "^0.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.5.0"
  }
}
